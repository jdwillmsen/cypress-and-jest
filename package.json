{
  "name": "cypress-and-jest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "jest": "jest",
    "ct": "ng run cypress-and-jest:ct",
    "e2e": "ng e2e",
    "ct:ci": "ng run cypress-and-jest:ct-ci",
    "e2e:ci": "ng run cypress-and-jest:e2e-ci",
    "jest:coverage": "jest --coverage",
    "cypress:coverage": "npx nyc report --reporter=html --reporter=lcov ---reporter=text --reporter=text-summary",
    "clean:coverage": "rm -rf coverage",
    "merge:coverage": "npx istanbul-merge --out coverage/merged-report/coverage-final.json coverage/jest/coverage-final.json coverage/cypress/coverage-final.json",
    "all:coverage": "npx nyc report --report-dir=coverage/merged-report --temp-dir=coverage/merged-report --reporter=html --reporter=lcov --reporter=text --reporter=text-summary --include=src/**/*.ts --exclude=src/main.ts",
    "tests:all": "npm run jest:coverage && npm run ct:ci && npm run e2e:ci && npm run merge:coverage && npm run all:coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@cypress/code-coverage": "^3.12.1",
    "@cypress/schematic": "^2.5.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.1",
    "cypress": "13.3.0",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.1",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.3",
    "webpack": "^5.88.2"
  }
}
